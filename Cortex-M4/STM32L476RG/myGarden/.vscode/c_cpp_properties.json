{
    "configurations": [
        {
            "name": "STM32",
            "includePath": [
                "${workspaceFolder}/Configuration/*",
                "${workspaceFolder}/Common/Garden/*",
                "${workspaceFolder}/Common/Hardware/*",
                "${workspaceFolder}/STM32/Hardware/*",
                "${workspaceFolder}/STM32/FreeRTOS/*",
                "${workspaceFolder}/STM32/BSP/STM32L4xx_Nucleo/*",
                "${workspaceFolder}/STM32/CMSIS/Include/*",
                "${workspaceFolder}/STM32/CMSIS/Device/ST/STM32L4xx/Include/*",
                "${workspaceFolder}/STM32/STM32L4xx_HAL_Driver/Inc/*",
                "${workspaceFolder}/FreeRTOS/Source/include/*",
                "${workspaceFolder}/FreeRTOS/Source/portable/GCC/ARM_CM4F/*",
                "${workspaceFolder}/FreeRTOS/Source/portable/MemMang/*",
                "C:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2021.07/arm-none-eabi/include/*",
                "C:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2021.07/arm-none-eabi/*"
            ],
            "defines": [
                "__HEAP_SIZE=0x200",
                "__STACK_SIZE=0x400",
                "ARM_MATH_CM4",
